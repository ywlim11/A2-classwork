phone_case_options = [
    "Leather Wallet Case - Burgundy (iPhone 13)",
    "Silicone Soft Case - Lime Green (Samsung Galaxy S21)",
    "Rugged Armor Case - Gunmetal Gray (Google Pixel 6)",
    "Clear Shockproof Case - Transparent (iPhone 12 Pro Max)",

# Task 1: Accessing Values
print("Task 1")
print(top_cs_universities["Stanford University"])
print(top_cs_universities["University of Oxford"])

# Task 2: Adding New Entries
print("Task 2")
top_cs_universities['University of Tokyo'] = 'Tokyo', 'Japan'
print(top_cs_universities)
# Task 3: Modifying Values
print("Task 3")
top_cs_universities['University of Cambridge'] = 'Cambridge, England'
print(top_cs_universities["University of Cambridge"])

# Task 4: Removing Entries
print("Task 4")
del top_cs_universities['Harvard University']
print(top_cs_universities)

# Task 5: Looping Through the Dictionary
print("Task 5")
for key in top_cs_universities:
    print("Key = " + key + ", Value = " + str(top_cs_universities[key]))

# Task 6: Dictionary Methods
print("Task 6")
for key in top_cs_universities:
    print("University: " + key)

for value in top_cs_universities.values():
    print("City: " + value[0] + ", Country: " + value[1])

for key in top_cs_universities:
    print("University: " + key + ", City: " + top_cs_universities[key][0] + ", Country: " + top_cs_universities[key][1])
# Task 7: Using get() Method
print("Task 7")
print(top_cs_universities.get("University of Tokyo"))
print(top_cs_universities.get("Unknown University"))

# Task 8: Updating Dictionary with update()
print("Task 8")
new_universities = {
    "University of California, Berkeley (UCB)": ("Berkeley", "USA"),
    "University of Illinois at Urbana-Champaign": ("Champaign", "USA")
}

top_cs_universities.update(new_universities)

# Printing 20 random names

import json
import random

# Load the JSON file
with open('names.json', 'r') as file:
    names_dict = json.load(file)

# 20 random names from the dictionary
selected_names = random.sample(list(names_dict.items()), 20)

for name, details in selected_names:
    print(f"Name: {name}")
    print(f"  Gender: {details['gender']}")
    print(f"  Country: {details['country']}")
    print() # Print a blank line for better readability

# Task 9: Dictionary Comprehension
print("Task 9")
uk_universities={"Imperial College London": ("London", "UK"), "University College London (UCL)": ("London", "UK"),"University of Cambridge": ("Cambridge", "UK"), "University of Edinburgh": ("Edinburgh", "UK"),"University of Warwick": ("Coventry", "UK"),"University of Oxford": ("Oxford", "UK")}
print(uk_universities)
